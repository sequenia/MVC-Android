<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sequenia.mvc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activities.InfoActivity">
        </activity>

        <activity android:name=".activities.InfoListActivity">
        </activity>

        <activity android:name=".activities.InfoListWithCacheActivity">
        </activity>

        <activity android:name=".activities.InfoPaginationListActivity">
        </activity>

        <activity android:name=".activities.InfoPaginationWithCacheActivity">
        </activity>

    </application>

</manifest>